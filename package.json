        "date-fns-tz": "^3.2.0",
        "dompurify": "^3.3.1",
        "framer-motion": "^12.23.26",
        "next": "15.2.8",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-query": "^3.39.3",
        "react-select": "^5.10.2",
        "scramble-display": "^0.6.8",
        "sr-scrambler": "^2.0.2"
